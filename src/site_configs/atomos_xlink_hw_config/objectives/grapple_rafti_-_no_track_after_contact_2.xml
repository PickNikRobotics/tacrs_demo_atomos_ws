<root BTCPP_format="4" main_tree_to_execute="Grapple RAFTI - No track -mock py script">
  <!--//////////-->
  <BehaviorTree ID="Grapple RAFTI - No track -mock py script" _description="Manual decay of last known velocity once gripper has &quot;soft&quot; closed (jaws are around RAFTI, but not yet fully closed.) Dup objective to test in mock with publish_odom_demo.py script." _favorite="true" _subtreeOnly="false">
    <Control ID="Sequence" name="TopLevelSequence">
      <SubTree ID="Open Gripper" _collapsed="true"/>
      <Action ID="CreateStampedPose" stamped_pose="{error_pose}" position_xyz="100.0;100.0;100.0;" reference_frame="world" name="Init to large error"/>
      <Action ID="ActivateControllers" controller_names="/velocity_force_controller"/>
      <Action ID="CreateStampedPose" stamped_pose="{target_pose_offset}" orientation_xyzw="0.5;0.5;0.5;0.5" position_xyz="0;0.0;0.0" reference_frame="world" name="target pose offset"/>
      <Action ID="CreateStampedWrench" torque_xyz="0.0;0.0;0.0" force_xyz="0.0;0.0;0.0" stamped_wrench="{desired_wrench}" name="Always 0, not using Force" reference_frame="world"/>
      <Action ID="CreateStampedTwist" name="Init 0 Pose (until FF-VFC outputs)" angular_velocity_xyz="0;0;0" linear_velocity_xyz="0;0;0" stamped_twist="{desired_twist}" reference_frame="world"/>
      <Control ID="Parallel" failure_count="1" success_count="1">
        <Decorator ID="KeepRunningUntilFailure">
          <Action ID="GetOdom" odometry_topic_name="odom_filtered" odometry_pose="{odometry_pose}" subscribed_odometry="{subscribed_odometry}"/>
        </Decorator>
        <Control ID="Sequence" name="TopLevelSequence">
          <Decorator ID="Repeat" num_cycles="-1">
            <Control ID="Sequence" name="TopLevelSequence">
              <Action ID="WaitForDuration" name="needed so agent doesn't crash" delay_duration="0.1"/>
              <Action ID="VisualizePose" name="odom pose for debug" pose="{odometry_pose}" marker_name="odom_pose" marker_lifetime="0.000000" marker_size="0.100000"/>
            </Control>
          </Decorator>
        </Control>
        <Control ID="Sequence" name="TopLevelSequence">
          <Decorator ID="KeepRunningUntilFailure">
            <Action ID="PublishVelocityForceCommand" wrench_gain="0.1" velocity_controlled_axes="1;1;1;1;1;1" force_controlled_axes="0;0;0;0;0;0" publish_rate="10" twist_stamped="{desired_twist}" velocity_force_controller_command_topic="/pose_jog/xlink_arm" wrench_stamped="{desired_wrench}"/>
          </Decorator>
        </Control>
        <Decorator ID="KeepRunningUntilFailure">
          <Action ID="PoseJog" controller_names="velocity_force_controller" link_padding="0.010000" planning_group_names="xlink_arm" stop_safety_factor="1.200000"/>
        </Decorator>
        <Control ID="Sequence" name="TopLevelSequence">
          <Control ID="Parallel" failure_count="1" success_count="1">
            <Decorator ID="Repeat" num_cycles="-1">
              <Control ID="Sequence">
                <Action ID="WaitForDuration" name="needed so agent doesn't crash" delay_duration="0.1"/>
                <Action ID="ComputeVelocityToAlignWithTarget" output_control_velocity="{desired_twist}" proportional_gain_angular="0.5" proportional_gain_linear="0.4" end_effector_frame="grasp_link" target_motion_state="{subscribed_odometry}" target_pose_offset="{target_pose_offset}" output_pose_error="{error_pose}"/>
              </Control>
            </Decorator>
            <Control ID="Sequence" name="TopLevelSequence">
              <Decorator ID="RetryUntilSuccessful" num_attempts="-1">
                <Control ID="Sequence">
                  <Action ID="WaitForDuration" name="needed so agent doesn't crash" delay_duration="0.1"/>
                  <Action ID="IsPoseNearIdentity" pose="{error_pose}" position_tolerance="0.02" rotation_tolerance="0.2"/>
                </Control>
              </Decorator>
              <Action ID="CreateStampedPose" reference_frame="world" stamped_pose="{target_pose_offset}" orientation_xyzw="0.5;0.5;0.5;0.5" position_xyz="0;0;0.0"/>
              <Decorator ID="RetryUntilSuccessful" num_attempts="-1">
                <Control ID="Sequence">
                  <Action ID="WaitForDuration" name="needed so agent doesn't crash" delay_duration="0.1"/>
                  <Action ID="IsPoseNearIdentity" pose="{error_pose}" position_tolerance="0.01" rotation_tolerance="0.1"/>
                </Control>
              </Decorator>
            </Control>
          </Control>
          <Control ID="Parallel" failure_count="1" success_count="2">
            <Control ID="Sequence" name="TopLevelSequence">
              <Action ID="Script" code="desired_twist2 := desired_twist"/>
              <Control ID="Fallback">
                <SubTree ID="Close Gripper" _collapsed="true"/>
                <Action ID="AlwaysSuccess"/>
              </Control>
            </Control>
            <Control ID="Sequence">
              <Control ID="Fallback">
                <Decorator ID="Timeout" msec="5000">
                  <Decorator ID="Repeat" num_cycles="-1">
                    <Control ID="Sequence">
                      <Action ID="WaitForDuration" name="needed so agent doesn't crash" delay_duration="0.1"/>
                      <Action ID="ComputeVelocityToAlignWithTarget" output_control_velocity="{desired_twist}" proportional_gain_angular="0.5" proportional_gain_linear="0.4" end_effector_frame="grasp_link" target_motion_state="{subscribed_odometry}" target_pose_offset="{target_pose_offset}" output_pose_error="{error_pose}"/>
                    </Control>
                  </Decorator>
                </Decorator>
                <Action ID="AlwaysSuccess"/>
              </Control>
              <Action ID="Script" code="desired_twist = desired_twist2"/>
              <Decorator ID="Repeat" num_cycles="100" name="This * time_step = total decel time. Set to 100 for simplicity.">
                <Control ID="Sequence" name="">
                  <Action ID="WaitForDuration" name="time_step for reducing velocity. Set to desired duration (s) / num_cycles." delay_duration="0.05"/>
                  <Action ID="ScaleTwist" input_velocity="{desired_twist}" scaled_velocity="{desired_twist}" velocity_scale="0.97"/>
                </Control>
              </Decorator>
            </Control>
          </Control>
        </Control>
      </Control>
      <Action ID="WaitForDuration" delay_duration="0.01"/>
      <SubTree ID="Open Gripper" _collapsed="true"/>
    </Control>
  </BehaviorTree>
  <TreeNodesModel>
    <SubTree ID="Grapple RAFTI - No track after contact 2">
      <MetadataFields>
        <Metadata runnable="true"/>
      </MetadataFields>
    <MetadataFields>
        <Metadata runnable="true"/>
      </MetadataFields></SubTree>
  </TreeNodesModel>
</root>